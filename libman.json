{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/teams-js@1.9.0",
      "destination": "wwwroot/lib/microsoft/teams-js/",
      "files": [
        "dist/MicrosoftTeams.min.js",
        "dist/MicrosoftTeams.js.map"
      ]
    },
    {
      "provider": "cdnjs",
